<!DOCTYPE html>
<html>

<body>
    <h1>health bot test 1</h1>

    <canvas id="myCanvas" width="300" height="150" style="border:1px solid black">
    </canvas>

    <script>
        const canvas = document.getElementById("myCanvas");
        const ctx = canvas.getContext("2d");

        //bot
        // body
        ctx.beginPath();
        ctx.arc(40, 40, 25, 0, 2 * Math.PI);
        ctx.fillStyle = "grey";
        ctx.fill();
        ctx.lineWidth = 4;
        //eyes
        ctx.beginPath();
        ctx.moveTo(22, 40);
        ctx.lineTo(58, 40);
        ctx.lineWidth = 10;
        ctx.lineCap = "round";
        ctx.stroke();
        ctx.beginPath();
        ctx.moveTo(24, 40);
        ctx.lineTo(56, 40);
        ctx.lineWidth = 5;
        ctx.lineCap = "round";
        ctx.strokeStyle = "grey";
        ctx.stroke();
        //headsetl
        ctx.beginPath();
        ctx.lineWidth = 4;
        ctx.strokeStyle = "black";
        ctx.arc(16, 40, 10, -1.6, 0.5 * Math.PI, true);
        ctx.fillStyle = "black";
        ctx.fill();
        ctx.stroke();
        //headsetr
        ctx.beginPath();
        ctx.arc(64, 40, 10, 1.6, -0.5 * Math.PI, true);
        ctx.fillStyle = "black";
        ctx.fill();
        ctx.stroke();
        //atenna
        ctx.beginPath();
        ctx.moveTo(66, 30);
        ctx.lineTo(66, 10);
        ctx.lineWidth = 2;
        ctx.lineCap = "round";
        ctx.strokeStyle = "black";
        ctx.stroke();
    </script>

</body>

</html>